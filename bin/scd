#!/bin/bash

# FZF or Skim
if command sk -h >/dev/null 2>&1; then
    fuzzy=sk
elif command fzf -h >/dev/null 2>&1; then
    fuzzy=fzf
else
    echo "You need skim or fzf for this function"
    exit 1
fi

if true; then
    echo todo
    # wd $(wd list | tail -n +2 | awk '{if (length($2) > 0) print $2}' | $fuzzy -q "$@")
else
    echo "You need wd for this function"
    exit 2
fi
